<div class="page-header">
  <h3>Meal Recipe #<%= @meal_recipe.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Meal</dt>
      <dd><%= @meal_recipe.meal_id %></dd>

      <dt>Recipe</dt>
      <dd><%= @meal_recipe.recipe_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/meal_recipes" class="btn btn-primary">
        Back
      </a>
      <a href="/meal_recipes/<%= @meal_recipe.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_meal_recipe/<%= @meal_recipe.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>

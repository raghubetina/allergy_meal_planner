<div class="page-header">
  <h1>
    Sensitivies
    <a href="/sensitivies/new" class="btn btn-lg btn-success">New Sensitivy</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Actions</th>
      </tr>

      <% @sensitivies.each do |sensitivy| %>
      <tr>
        <td><%= sensitivy.name %></td>
        <td>
          <a href="/sensitivies/<%= sensitivy.id %>" class="btn btn-primary">Show</a>
          <a href="/sensitivies/<%= sensitivy.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_sensitivy/<%= sensitivy.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @sensitivies, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
